{
	"cpu": "cortex-a8",
	"llvm-target": "arm-unknown-none-gnueabi",
	"target-endian": "little",
	"target-pointer-width": "32",
	"target-word-size": "32",
	"os": "tifflin",
	"arch": "arm",
		"linker": "arm-elf-eabi-gcc",
		"pre-link-args": ["-nostdlib", "-ffreestanding", "-Wl,-T,link-armv7.ld", "-Wl,-z,max-page-size=4096", "-Wl,--gc-sections", ".objs/armv7/rustrt0.o", "-Wl,--start-group"],
		"post-link-args": ["-Wl,--end-group"],
		"no-compiler-rt": true,
		"features": "+v7",
		"executables": true,
		"dynamic-linking": true,
	"morestack": false
}
